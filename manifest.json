  {
    "name": "File Downloader sample",
    "manifest_version": 2,
    "version": "1.0.0",
    "content_scripts": [{
      "matches": ["https://www.google.com/"],
      "js": ["content.js"]
    }],
    "background": {
        "scripts": ["background.js", "populator.js"],
        "persistent": false
      },
    "permissions": ["activeTab", "storage", "http://hello/*", "https://*/*"]
  }
  