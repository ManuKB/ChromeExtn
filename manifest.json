  {
    "name": "File Downloader sample",
    "manifest_version": 3,
    "version": "1.0.0",
    "content_scripts": [{
      "matches": ["https://www.google.com/"],
      "js": ["populator.js","content.js"]
    }],
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["activeTab", "storage"],
    "host_permissions": [
      "https://www.blogger.com/", "https://www.nseindia.com/*"
    ]
  }
  
